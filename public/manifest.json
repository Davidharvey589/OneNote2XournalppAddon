{
  "manifest_version": 3,
  "name": "OneNote to Xournal++",
  "description": "This addon converts OneNote notes to Xournal++ notes.",
  "version": "1.0.3",
  "author": "nico9889",
  "homepage_url": "https://github.com/nico9889/OneNote2XournalppAddon",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["*://onedrive.live.com/*","*://onenote.officeapps.live.com/*"],
  "content_scripts": [
    {
      "matches": ["*://onedrive.live.com/*","*://onenote.officeapps.live.com/*"],
      "all_frames": true,
      "js": ["content.js"]
    }
  ],
  "icons": {
    "255": "xournal.png"
  },
  "action": {
    "default_icon": {
      "255": "xournal.png"
    },
    "default_title": "Convert to Xournal++"
  },
  "background": {
    "scripts": ["onenote2xournalpp.js"]
  }
}
